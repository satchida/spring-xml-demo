<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context.xsd">


    <bean id="actor1" class="com.stackroute.domain.Actor">
        <property name="name" value="Akshay kumar"></property>
        <property name="gender" value="Male"></property>
        <property name="age" value="53"></property>
    </bean>
    <bean id="actor10" class="com.stackroute.domain.Actor">
        <property name="name" value="Akshay"></property>
        <property name="gender" value="Male"></property>
        <property name="age" value="43"></property>
    </bean>

    <bean id="Holiday" class="com.stackroute.domain.Movie">
        <property name="actor" ref="actor1"></property>
        <!--<property name="actor" ref="actor10"></property>-->
    </bean>


    <bean id="actor2" class="com.stackroute.domain.Actor">
        <property name="name" value="Salman Khan"></property>
        <property name="gender" value="Male"></property>
        <property name="age" value="61"></property>
    </bean>

    <!--<bean id="Wanted" class="com.stackroute.domain.Movie">
        <property name="actor" ref="actor2"></property>
    </bean>-->
     
</beans>

